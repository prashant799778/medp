<!--    <include android:id="@+id/toolbar" layout="@layout/toolbar_layout"/>-->

    <RelativeLayout
        android:id="@+id/toolbar"
        android:background="@color/colorPrimary"
        android:layout_width="match_parent"
        android:layout_height="52dp">
        <ImageButton
            android:id="@+id/bck"
            android:layout_width="50dp"
            android:padding="15dp"
            android:layout_height="match_parent"
            android:scaleType="fitCenter"
            android:background="@color/transparent"
            android:src="@drawable/ic_back"></ImageButton>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_centerInParent="true"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="30dp"
                android:src="@drawable/new_ic_logo"
                android:id="@+id/new_logo"
                android:layout_centerVertical="true"
                android:layout_height="25dp">
            </ImageView>
            <com.medparliament.Widget.Segow_UI_Bold_Font
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/MedParliament"
                android:layout_centerHorizontal="true"
                android:textSize="18dp"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/new_logo"
                android:textColor="@color/white"
                android:textStyle="bold">
            </com.medparliament.Widget.Segow_UI_Bold_Font>
        </RelativeLayout>
    </RelativeLayout>


    -<RelativeLayout android:id="@+id/tab_relative_layout" android:layout_height="match_parent" android:layout_width="match_parent" android:orientation="vertical" android:layout_below="@+id/toolbar">

    <com.google.android.material.tabs.TabLayout android:id="@+id/tabs" android:layout_height="wrap_content" android:layout_width="match_parent" app:tabMode="scrollable" app:tabGravity="fill" app:tabTextColor="@color/white" app:tabBackground="@color/colorPrimary" app:tabIndicatorColor="@color/white" app:tabSelectedTextColor="@color/white" app:tabTextAppearance="@style/MyCustomTabText" android:background="@color/colorPrimary"/>

    <com.medparliament.Widget.CustomViewPageNew
        android:id="@+id/viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/tabs"
        android:layout_marginTop="0dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:visibility="invisible"
        style="@style/Widget.MaterialComponents.FloatingActionButton"
        android:scaleType="centerCrop"
        android:layout_marginRight="30dp"
        app:backgroundTint="@color/white"
        app:rippleColor="@color/colorAccent"
        android:layout_marginBottom="50dp"
        android:id="@+id/cmnt"
        android:src="@drawable/new_post"
        app:tint="@null"
        android:layout_height="wrap_content">
    </com.google.android.material.floatingactionbutton.FloatingActionButton>
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_alignParentRight="true"
        android:visibility="visible"
        style="@style/Widget.MaterialComponents.FloatingActionButton"
        android:scaleType="centerCrop"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="10dp"
        app:backgroundTint="@color/white"
        app:rippleColor="@color/colorAccent"
        android:layout_above="@id/cmnt"
        android:id="@+id/share"
        android:src="@drawable/ic_share"
        app:tint="@null"
        android:layout_height="wrap_content">
    </com.google.android.material.floatingactionbutton.FloatingActionButton>


</RelativeLayout>

